<%#Seccion3: Indicaciones descarga PWA%>

<div class="comp-title">
    	<h2>¿Como funciona BiBi?</h2>
</div>
<div class="infobibpas">
	<div class="bibpasder">
		<div class="pastexpasder">
			<div class="pastextiteder">
				<h3 class="pastextit">Registrate</h3>
				<p class="parbibtex">Ingresa todos los datos necesarios para poder rentar con BiBi.</p>
				<button class="btnpas" type="submit"><%= link_to "Registrarme", user_session_path, "data-toggle" => "modal", "data-target" => "#signinModal" %></button>
			</div>
		</div>
	</div>
	<div class="bibpasizq">
		<div class="pastexpasizq">
			<div class="pastextiteizq">
				<h3 class="pastextit" >Explora</h3>
				<p class="parbibtex">Busca tu artículo por ubicación o categoria.</p>
				<button class="btnpas" type="submit"><%= link_to "Explorar", products_path %></button>
			</div>
		</div>
	</div>
	<div class="bibpasder2">
		<div class="pastexpasder">
			<div class="pastextiteder">
				<h3 class="pastextit">Disfruta</h3>
				<p class="parbibtex">Disfruta del artículo el tiempo que lo necesites.</p>
			</div>
		</div>
	</div>
</div>

